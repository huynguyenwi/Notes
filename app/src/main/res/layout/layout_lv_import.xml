<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <View
        android:id="@+id/view"
        android:layout_width="30dp"
        android:layout_height="35dp"
        android:layout_marginTop="10dp"
        android:layout_marginStart="10dp"
        android:layout_marginBottom="10dp"
        android:background="@drawable/baseline_feed_24" />

    <TextView
        android:id="@+id/textview_importfile"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="20dp"
        android:layout_marginTop="5dp"
        android:layout_marginEnd="20dp"
        android:layout_toStartOf="@+id/btncheck"
        android:layout_toEndOf="@+id/view"
        android:layout_toRightOf="@+id/view"
        android:text="Notes-20231109T203555.ocn"
        android:textSize="19dp" />

    <Button
        android:id="@+id/btncheck"
        android:layout_width="25dp"
        android:layout_height="25dp"
        android:layout_alignParentEnd="true"
        android:layout_marginTop="15dp"
        android:layout_marginEnd="10dp"
        android:background="@drawable/baseline_check_circle_gray"
        app:backgroundTint="@null"/>

</RelativeLayout>